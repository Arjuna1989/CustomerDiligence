
@{
    ViewBag.Title = "Customer Diligence";
}
@*,'@ViewBag.Id','@ViewBag.AmlGroupId'*@
<div id="content" ng-controller="CreateDiligenceController" ng-init="Init('@ViewBag.LoggedUser.AgentId','@ViewBag.LoggedUser.GroupId','@ViewBag.LoggedUser.CountryCode')">

    <div id="loader" class="preloader-area">
        <div class="spinner-gears"></div>
    </div>
    <div class="modal" id="customerRecord" tabindex="-1">
        <div class="modal-dialog" style="width:1250px;">
            <div class="modal-content">

                <div class="modal-header">
                    <button class="close" data-dismiss="modal">&times;</button>
                    <center> <h4 class="modal-title"><b>Customer Record </b></h4> </center>
                </div>

                <div class="modal-body" ng-repeat="cust in DiligenceCustomer">
                    <fieldset>
                        <legend><b>Record {{$index+1}} </b></legend>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>First Name</b> </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.FirstName}} " readonly class="form-control">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Second name</b>  </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.SecondName}}" readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Third Name</b>   </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.ThirdName}} " readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Comment  </b>  </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.Commect}}" readonly class="form-control">

                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Ref Number</b>  </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.RefNumber}}" readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Desingnation</b> </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.Designation}}" readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Nationality</b> </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.Nationality}} " readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>LastUpdateDate</b> </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.LastUpdateDate}}" readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Alias</b> </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.Alias}}" readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Address</b> </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.Address}}" readonly class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="textinput"><b>Birthday</b> </label>
                            <div class="col-sm-10">
                                <input type="text" value="{{cust.Birthday}}" readonly class="form-control">
                            </div>
                        </div>
                    </fieldset>
                </div>

                <div class="modal-footer">
                    <button class="btn btn-primary" type="button" data-dismiss="modal"> Close </button>
                </div>

            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-xs-12 col-sm-7 col-md-7 col-lg-4">
